{
  "name": "@kyudiscord/blade",
  "version": "1.0.10",
  "description": "A powerful bot framework for the Neo Discord Library.",
  "main": "dist/index.js",
  "repository": "https://github.com/kyudiscord/neo",
  "author": "MeLike2D",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rm -rf dist && tsc --removeComments",
    "build:types": "rm -rf dist && tsc -d && yarn dts-bundle",
    "dts-bundle": "dts-bundle --removeSource --name @kyudiscord/blade --main ./dist --out ../index.d.ts --baseDir ./dist --outputAsModuleFolder"
  },
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@kyudiscord/neo": "^1.0.18",
    "@melike2d/logger": "^1.0.4",
    "tslib": "^2.0.1"
  }
}
